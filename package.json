{
  "private": true,
  "name": "psim",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "rollup -cw",
    "build": "rm -rf dist/* && NODE_ENV=production rollup -c",
    "lint": "eslint ."
  },
  "browserslist": [
    "last 2 versions and not dead and > 0.5% in US",
    "not ie 11",
    "ios >=9.3"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    }
  },
  "eslintIgnore": [
    "www",
    "rollup.config.js"
  ],
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "cordova": "^9.0.0",
    "cordova-android": "^8.0.0",
    "gameboy-sound": "0.0.2",
    "pxcan": "0.0.6",
    "rollup": "^1.11.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-url": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.5.0",
    "if-env": "^1.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-serve": "^1.0.1"
  },
  "cordova": {
    "platforms": [
      "android"
    ]
  }
}